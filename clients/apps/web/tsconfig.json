{
  "extends": "tsconfig/nextjs.json",
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "strict": true,
    "baseUrl": "src/",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "next.d.ts", "src/**/*.ts", "src/**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
