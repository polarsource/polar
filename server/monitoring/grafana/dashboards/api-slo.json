      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "expr": "100 * clamp_min(1 - ((sum(increase(polar_http_request_total{env=\"$env\", status_code=~\"5..\"}[$__range])) or vector(0)) / clamp_min(0.0005 * sum(increase(polar_http_request_total{env=\"$env\"}[$__range])), 0.001)), 0)",
