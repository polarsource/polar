      "transformations": [
        {
          "id": "joinByField",
          "options": {
            "byField": "endpoint",
            "mode": "outer"
          }
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "Time 1": true,
              "Time 2": true,
              "Time 3": true,
              "Time 4": true,
              "method 2": true,
              "method 3": true,
              "method 4": true
            },
            "indexByName": {
              "endpoint": 0,
              "method 1": 1,
              "Value #P99Breach": 2,
              "Value #P99Target": 3,
              "Value #AvailBreach": 4,
              "Value #AvailTarget": 5
            },
            "renameByName": {
              "endpoint": "Endpoint",
              "method 1": "Method",
              "Value #P99Breach": "Current P99",
              "Value #P99Target": "Target P99",
              "Value #AvailBreach": "Current Avail",
              "Value #AvailTarget": "Target Avail"
            }
          }
        },
        {
          "id": "filterByValue",
          "options": {
            "filters": [
              {
                "config": {
                  "id": "isNotNull",
                  "options": {}
                },
                "fieldName": "Current P99"
              },
              {
                "config": {
                  "id": "isNotNull",
                  "options": {}
                },
                "fieldName": "Current Avail"
              }
            ],
            "match": "any",
            "type": "include"
          }
        },
        {
          "id": "calculateField",
          "options": {
            "alias": "Issue",
            "mode": "binary",
            "reduce": {
              "reducer": "sum"
            },
            "binary": {
              "left": "Current P99",
              "operator": ">",
              "reducer": "sum",
              "right": "0"
            }
          }
        }
      ],
      "type": "table"
    },
